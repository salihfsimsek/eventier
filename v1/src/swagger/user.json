{
  "tags": ["Users"],
  "description": "Returns all users from the system that the user has access to",
  "operationId": "getUsers",
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "responses": {
    "200": {
      "description": "A list of users.",
      "content": {
        "application/json": {
          "schema": {
            "type": "array",
            "items": {
              "username": {
                "type": "string"
              },
              "full_name": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
